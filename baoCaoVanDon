function () {
    showLoader();
    this.showError('Đang tải dữ liệu, vui lòng chờ...');
    this.updateCharts([]);

    // URL của API không cần thêm tham số callback nữa
    // Nếu URL của bạn có sẵn "?tableName=F3", hãy giữ nguyên nó
    const apiUrl = 'https://script.google.com/macros/s/AKfycbwxYvbxWphp7FHtG4CvHX8ZeIF-pz58w2j8XvgGsxASmC8nWaWjwDAjrh3Ndpnb5Qg/exec?tableName=F3';

    fetch(apiUrl)
        .then(response => {
            // Kiểm tra xem yêu cầu có thành công không (status code 200-299)
            if (!response.ok) {
                // Nếu không thành công,โยน lỗi để nhảy vào khối .catch()
                throw new Error(`Lỗi mạng: ${response.statusText}`);
            }
            // Chuyển đổi phản hồi sang JSON
            return response.json();
        })
        .then(data => {
            // Gọi hàm xử lý dữ liệu với JSON đã nhận được
            this.handleData(data);
        })
        .catch(error => {
            // Bắt và hiển thị bất kỳ lỗi nào xảy ra trong quá trình fetch
            console.error('Lỗi khi tải dữ liệu:', error);
            this.showError('Tải dữ liệu thất bại. Vui lòng kiểm tra kết nối mạng, đường dẫn API và thử lại.');
        });
}
